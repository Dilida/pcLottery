/**
 * 覆蓋quasar預設變數（顏色為主）
 * 注意：勿更改或刪除變數名稱，僅更改值
 * https://quasar-framework.org/components/stylus-variables.html
 */
$primary   = #19A863
$secondary   = #19A863

/**
 * 自訂default版顏色
 */
$toolbar1 = #15854F
$toolbar2 = #1DB96E

/* 导航渐层色 */
$color1 = #47CB8B
$color2 = #47CB8B
$color3 = #19A863
$color4 = #19A863

$primary-bright = #59D181
$primary-middle = #35C06E
$primary-strong = #0DA75D
$primary-pale = rgba($primary, 0.1)

$money = #39B077
$notice = #FF8147

/**
 * 共用圖片路徑
 */
$styleFolder = $path + 'statics/default/default009/'
$styleUrl(v)
    url($styleFolder + (v))