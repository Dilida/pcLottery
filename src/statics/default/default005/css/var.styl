/**
 * 覆蓋quasar預設變數（顏色為主）
 * 注意：勿更改或刪除變數名稱，僅更改值
 * https://quasar-framework.org/components/stylus-variables.html
 */
$primary   = #2545A0
$secondary   = #2545A0

/**
 * 自訂default版顏色
 */
$toolbar1 = #071E5E
$toolbar2 = #2545A0

/* 导航渐层色 */
$color1 = #3860D2
$color2 = #446de4
$color3 = #2953c7
$color4 = #2545A0

$primary-bright = #436BDC
$primary-middle = #2851C5
$primary-strong = #1A347C
$primary-pale = rgba($primary, 0.1)

$money = #3B61CF
$notice = #FFC200

/**
 * 共用圖片路徑
 */
$styleFolder = $path + 'statics/default/default005/'
$styleUrl(v)
    url($styleFolder + (v))