/**
 * 覆蓋quasar預設變數（顏色為主）
 * 注意：勿更改或刪除變數名稱，僅更改值
 * https://quasar-framework.org/components/stylus-variables.html
 */
$primary   = #D7006C
$secondary   = #D7006C

/**
 * 自訂default版顏色
 */
$toolbar1 = #BF0C66
$toolbar2 = #FA429E

/* 导航渐层色 */
$color1 = #FF59A5
$color2 = #ff59a5
$color3 = #dc2681
$color4 = #D7006C


$primary-bright = #D7006C
$primary-middle = #BF0060
$primary-strong = #8A0045
$primary-pale = rgba($primary, 0.1)

$money = #D91F7C
$notice = #FFEC00

/**
 * 共用圖片路徑
 */
$styleFolder = $path + 'statics/default/default002/'
$styleUrl(v)
    url($styleFolder + (v))