/**
 * 覆蓋quasar預設變數（顏色為主）
 * 注意：勿更改或刪除變數名稱，僅更改值
 * https://quasar-framework.org/components/stylus-variables.html
 */
$primary   = #127ECE
$secondary = #127ECE

/**
 * 自訂default版顏色
 */
/* toolbar 渐层按钮 */
$toolbar1 = #127ECE
$toolbar2 = #1DB5F7

/* 导航渐层色 */
$color1 = #1DB5F7
$color2 = #1798E2
$color3 = #1798E2
$color4 = #127ECE


$primary-bright = #1DB5F7
$primary-middle = #00A0E6
$primary-strong = #127ECE
$primary-pale = rgba(#D0F1FF, 0.5)
$primary-vivid-1 = $primary-pale

$money = #FF4949
$notice = #FFEC00

/**
 * 共用圖片路徑
 */
$styleFolder = $path + 'statics/default/default902/'
$styleUrl(v)
    url($styleFolder + (v))