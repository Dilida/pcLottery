/**
 * 覆蓋quasar預設變數（顏色為主）
 * 注意：勿更改或刪除變數名稱，僅更改值
 * https://quasar-framework.org/components/stylus-variables.html
 */
$primary   = #F78232
$secondary   = #F78232

/**
 * 自訂default版顏色
 */
$toolbar1 = #E15401
$toolbar2 = #FD9F5E

/* 导航渐层色 */
$color1 = #FF8C3D
$color2 = #FF8C3D
$color3 = #FF782B
$color4 = #FF782B

$primary-bright = #FFA46B
$primary-middle = #FF8C4B
$primary-strong = #ff782b
$primary-pale = rgba($primary, 0.1)

$money = #FF9146
$notice = #717AFF

/**
 * 共用圖片路徑
 */
$styleFolder = $path + 'statics/default/default010/'
$styleUrl(v)
    url($styleFolder + (v))